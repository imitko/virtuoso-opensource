virtrdf:GqlTypesData a void:Dataset ;
    rdfs:label "GraphQL to SPARQL Bridge Core Types Dataset" ;
    rdfs:comment """
            Core Dataset for GraphQL to SPARQL bridge.
            It must be loaded in `urn:graphql:intro:core` RDF graph, for mappings see `urn:graphql:schema`
            """ ;
    schema:url <http://www.openlinksw.com/graphql/graphql-intro.ttl> ;
    virtrdf:version "0.9.1" ;
    dct:created "2022-09-10"^^xsd:date ;
    dct:modified "2022-09-10"^^xsd:date .

:__schema rdf:type gql:__schema_Class ;
                :queryType :Query;
                :mutationType :Mutation;
                :subscriptionType rdf:nil;
                :types :String, :Float, :Int, :Boolean, :DateTime, :IRI, :__Schema, :__Type, :__TypeKind,
                       :__Field, :__InputValue, :__EnumValue, :__Directive, :__DirectiveLocation, :Query, :Mutation ;
                :directives :embedded, :specifiedBy, :deprecated, :skip, :include .

:deprecated rdf:type gql:deprecated_Class;
                :name "deprecated";
                :locations "ARGUMENT_DEFINITION", "INPUT_FIELD_DEFINITION", "ENUM_VALUE", "FIELD_DEFINITION" ;
                :description "Marks an element of a GraphQL schema as no longer supported" ;
                :args :reason .

:skip rdf:type gql:skip_Class ;
                :name "skip" ;
                :locations "FRAGMENT_SPREAD", "INLINE_FRAGMENT", "FIELD" ;
                :description "Directs the executor to skip this field or fragment when the `if` argument is true.";
                :args :if .

:include rdf:type gql:include_Class ;
               :name "include" ;
               :locations "FRAGMENT_SPREAD", "INLINE_FRAGMENT", "FIELD" ;
               :description "Directs the executor to skip this field or fragment when the `if` argument is true" ;
               :args :if .

